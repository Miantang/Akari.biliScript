/* Sample: HelloWorld
 * A minimalist sample that displays a single line of that famous quote on the screen for a minute.
 */

( Akari = Global._get( "__akari" ) ).execute( Akari.Display.MainComposition({ layers : [ Akari.Display.Layer({ source : Akari.Display.Text(), inPoint : 0, outPoint : 60000, properties : { text : "Hello World!" } }) ] }) );